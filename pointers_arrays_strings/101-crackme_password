#include <stdio.h>

int main(void)
{
    FILE *f = fopen("101-crackme_password", "w");
    if (f == NULL)
    {
        perror("Failed to open file");
        return 1;
    }

    // Замените строку ниже на реальный пароль
    const char *password = "YOUR_PASSWORD";

    fprintf(f, "%s", password);  // без символа новой строки
    fclose(f);
    return 0;
}
